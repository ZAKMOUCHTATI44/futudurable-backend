{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "blog_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-comment.blog-comment",
      "mappedBy": "blog"
    },
    "dat": {
      "type": "date"
    },
    "author": {
      "type": "string"
    },
    "sous_category": {
      "type": "string"
    },
    "image_source": {
      "type": "string"
    }
  }
}
